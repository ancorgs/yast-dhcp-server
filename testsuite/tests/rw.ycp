{
    include "testsuite.ycp";
    import "Progress";
    import "DhcpServer";

    Progress::off ();

    map READ = $[
	// Runlevel
        "init": $[
            "scripts": $[
                "exists": true,
                "runlevel": $[
                    "dhcpd": $[
                        "start": [],
                        "stop":  [],
                        ],
                    ],
                // their contents is not important for ServiceAdjust
                "comment": $[
                    "dhcpd": $[],
                    ],
                ],
            ],

	"target" : $[
	    "stat" : $[],
	    "ycp" : $[],
	],
    ];
    map WRITE = $[];
    map EXEC = $[
	"target" : $[
	    "bash_output" : $[],
	],
    ];

    TEST (``(DhcpServer::Read ()), [READ, WRITE, EXEC], 0);
    TEST (``(DhcpServer::Write ()), [READ, WRITE, EXEC], 0);
}
